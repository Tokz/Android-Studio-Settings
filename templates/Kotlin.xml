<templateSet group="Kotlin">
  <template name="logd" value="if(BuildConfig.DEBUG) {&#10;    Log.d(&quot;$METHOD_NAME$&quot;,&quot;$content$&quot;)&#10;}" description="Log.d for Kotlin" toReformat="true" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="kotlinFunctionName()" defaultValue="" alwaysStopAt="true" />
    <variable name="content" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="KOTLIN_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>